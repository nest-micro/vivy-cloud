  "homepage": "https://github.com/bfehub/vivy-cloud",
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "commit": "npx git-cz",
    "lint:lint-staged": "lint-staged",
    "lint:eslint": "eslint --fix --cache \"**/*.{js,jsx,ts,tsx}\"",
    "docker:rm": "docker-compose -f docker/docker-compose.yaml rm",
    "docker:stop": "docker-compose -f docker/docker-compose.yaml stop",
    "docker:base": "docker-compose -f docker/docker-compose.yaml up -d vivy-mysql vivy-redis vivy-nacos"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@vivycloud/common-core": "workspace:*",
    "@vivycloud/common-swagger": "workspace:*",
    "@vivycloud/config": "workspace:*",
    "@vivycloud/config-nacos": "workspace:*",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@bfehub/eslint-config-typescript": "^1.3.3",
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.18",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.11.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "typescript": "^4.9.5"
  }
}
